(defun vscode-icons-clear-all-cache ()
  "Clear all VSCode icons cache."
  (interactive)
  (dolist (tbl (list vscode-icons-file-regex-icon-cache
                     vscode-icons-file-ext-icon-cache
                     vscode-icons-dir-icon-cache
                     vscode-icons-mode-icon-cache
